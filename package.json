{
  "name": "dashund",
  "version": "0.4.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "robb-j/dashund",
  "author": "Rob Anderson (https://r0b.io)",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "npx tsc --noEmit",
    "coverage": "jest --coverage",
    "preversion": "npm test -s && npm run build",
    "prettier": "prettier --write '**/*.{js,ts,tsx,json,css,md}'",
    "start": "node -r dotenv/config dist/index.js",
    "test": "jest",
    "dev:cli": "npm run build &> /dev/null; node example/cli.js",
    "generate-readme-toc": "md-toc -i"
  },
  "keywords": [],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ms": "^2.1.2",
    "prompts": "^2.3.1",
    "stoppable": "^1.1.0",
    "ws": "^7.2.3",
    "yaml": "^1.8.2",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@robb_j/md-toc": "^0.3.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/ms": "^0.7.31",
    "@types/node": "^13.9.1",
    "@types/prompts": "^2.0.5",
    "@types/stoppable": "^1.1.0",
    "@types/supertest": "^2.0.8",
    "@types/ws": "^7.2.2",
    "@types/yaml": "^1.2.0",
    "@types/yargs": "^15.0.4",
    "akita-ws": "^0.1.2",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "yorkie": "^2.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
